<Window x:Class="PetsRUs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PetsRUs"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtbusername" HorizontalAlignment="Center" Margin="0,179,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="180"/>
        <TextBox x:Name="txtbpass" HorizontalAlignment="Center" TextWrapping="Wrap" Text="" VerticalAlignment="Center" Width="180"/>
        <Button x:Name="btnLogin" Content="Login" HorizontalAlignment="Center" Margin="0,244,0,0" VerticalAlignment="Top" Width="120" Click="btnLogin_Click"/>
    </Grid>
</Window>
